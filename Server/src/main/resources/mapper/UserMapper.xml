<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.zqu.hr_system.project.mapper.UserMapper">

    <select id="selectByRostId" resultType="cn.edu.zqu.hr_system.project.model.entities.User">
        select *
        from user
        where pid = #{roleId}
    </select>
